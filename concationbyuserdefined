#include<stdio.h>
int main(){
    char p[20] = "Priyanshu";
    char g[30] = "Gaur";
    char d[50];
    int i = 0, j = 0;
    
    while(p[i] != '\0'){
        d[j] =p[i];
        i++;
        j++;
    }
    i = 0;
    while(g[i] != '\0' ){
        d[j] = g[i];
        i++;
        j++;
    }
    d[j] = '\0';
    printf("%s" , d);
    return 0 ;
}
